            </TransformGroup>
        </Grid.RenderTransform>
        <Rectangle Height="49" Stroke="Yellow"  VerticalAlignment="Top" RadiusY="12.5" RadiusX="12.5" Margin="-1,2,4,0"/>
        <TextBlock x:Name="Txt_Aviso" Margin="0,7,4,7" TextWrapping="Wrap" Text="NO Docente"  FontFamily="Verdana" FontWeight="Bold" FontSize="26.667" Foreground="Yellow"  TextAlignment="Center" />
    </Grid>
