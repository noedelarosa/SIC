<Window x:Class="Manager.Win_ReentrarClave"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Entre la clave nuevamente." Height="174" Width="406" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid>
        <Image HorizontalAlignment="Left" Height="47" Margin="10,10,0,0" VerticalAlignment="Top" Width="47" Source="/SIC;component/Objs/Comun/imgs/reload_48.png"/>
        <TextBlock Margin="62,10,10,0" TextWrapping="Wrap" Text="Entre la clave de usuario para confirmación." VerticalAlignment="Top" FontWeight="Bold" FontSize="16"/>
        <TextBlock HorizontalAlignment="Left" Margin="29,73,0,0" TextWrapping="Wrap" VerticalAlignment="Top"><Run Language="es-do" Text="Clave"/></TextBlock>
        <PasswordBox x:Name="Txt_Clave" Margin="71,69,10,0" VerticalAlignment="Top" MaxLength="15" Height="20"/>
        <Button x:Name="But_Aceptar" Content="Aceptar" HorizontalAlignment="Left" Margin="71,104,0,0" VerticalAlignment="Top" Width="87" Click="But_Aceptar_Click"/>
        <Button x:Name="But_Salir" Content="Salir" HorizontalAlignment="Left" Margin="163,104,0,0" VerticalAlignment="Top" Width="87"/>

    </Grid>
</Window>
